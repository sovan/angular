/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "paths": {
      "accordion": [
        "dist/accordion/accordion",
        "dist/accordion"
      ],
      "breadcrumb": [
        "dist/breadcrumb/breadcrumb",
        "dist/breadcrumb"
      ],
      "dropdown": [
        "dist/dropdown/dropdown",
        "dist/dropdown"
      ],
      "footer": [
        "dist/footer/footer",
        "dist/footer"
      ],
      "form": [
        "dist/form/form",
        "dist/form"
      ],
      "grid": [
        "dist/grid/grid",
        "dist/grid"
      ],
      "imagecard": [
        "dist/imagecard/imagecard",
        "dist/imagecard"
      ],
      "innerlayout": [
        "dist/innerlayout/innerlayout",
        "dist/innerlayout"
      ],
      "layout": [
        "dist/layout/layout",
        "dist/layout"
      ],
      "modal": [
        "dist/modal/modal",
        "dist/modal"
      ],
      "navbar": [
        "dist/navbar/navbar",
        "dist/navbar"
      ],
      "npm-packages": [
        "dist/npm-packages/npm-packages",
        "dist/npm-packages"
      ],
      "pagination": [
        "dist/pagination/pagination",
        "dist/pagination"
      ],
      "slider": [
        "dist/slider/slider",
        "dist/slider"
      ],
      "tabmenu": [
        "dist/tabmenu/tabmenu",
        "dist/tabmenu"
      ]
    },
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
